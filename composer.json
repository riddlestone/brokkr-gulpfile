{
    "name": "riddlestone/zf-gulpfile",
    "description": "A tool to create gulpfiles based on ZendFramework module configuration",
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/riddlestone/zf-gulpfile",
    "type": "library",
    "extra": {
        "zf": {
            "component": "Riddlestone\\ZF\\Gulpfile"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "authors": [
        {
            "name": "Andrew Riddlestone",
            "email": "andrew.riddlestone@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Riddlestone\\ZF\\Gulpfile\\": "src/"
        }
    },
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "riddlestone/zf-console": "^1.0.1",
        "zendframework/zend-servicemanager": "^3.4"
    },
    "autoload-dev": {
        "psr-4": {
            "Riddlestone\\ZF\\Gulpfile\\Test\\": "test/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    }
}
